<?xml version="1.0" encoding="UTF-8"?>

<?import org.controlsfx.control.GridView?><?import java.lang.String?><?import java.net.URL?><?import javafx.geometry.Insets?><?import javafx.scene.control.Button?><?import javafx.scene.control.ComboBox?><?import javafx.scene.control.Label?><?import javafx.scene.control.TableColumn?><?import javafx.scene.control.TableView?><?import javafx.scene.control.TextField?><?import javafx.scene.control.ToggleButton?><?import javafx.scene.control.ToggleGroup?><?import javafx.scene.layout.*?>
<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.172-ea"
  xmlns:fx="http://javafx.com/fxml/1"
>
  <children>
    <HBox prefHeight="60.0" prefWidth="1207.0" styleClass="box-topo" AnchorPane.leftAnchor="0.0"
      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Label fx:id="lbTitle" alignment="BOTTOM_LEFT" layoutX="10.0"
        maxWidth="1.7976931348623157E308" styleClass="titulo" text="Título"
        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="445.0"
        AnchorPane.topAnchor="0.0" HBox.hgrow="ALWAYS" />

        <ToggleButton mnemonicParsing="false" onAction="#tbAdd" text="">
          <styleClass>
            <String fx:value="menus-tela" />
            <String fx:value="menu-add" />
          </styleClass>
          <toggleGroup>
            <ToggleGroup fx:id="menu" />
          </toggleGroup>
        </ToggleButton>
        <ToggleButton layoutX="1150.0" layoutY="27.0" mnemonicParsing="false" onAction="#tbEdit"
          text="" toggleGroup="$menu">
          <styleClass>
            <String fx:value="menus-tela" />
            <String fx:value="menu-editar" />
          </styleClass>
        </ToggleButton>
      </children>
    </HBox>
    <StackPane AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="15.0"
      AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="60.0">
      <StackPane fx:id="nodeEdit">
        <AnchorPane fx:id="apProductList" visible="true" >
          <HBox AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <TextField fx:id="txtSearch" prefHeight="50.0" prefWidth="417.0" promptText="%txt_search"
              styleClass="search-txt-field" >
              <HBox.margin>
                <Insets right="10"/>
              </HBox.margin>
            </TextField>
            <Label styleClass="lb-info" text="Loại:" alignment="CENTER_LEFT" prefHeight="30">
              <HBox.margin>
                <Insets right="10"/>
              </HBox.margin>
            </Label>
            <ComboBox fx:id="cbFilter" prefHeight="30" />

          </HBox>
          <GridView AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0"
            AnchorPane.topAnchor="70.0" cellWidth="150" cellHeight="200" fx:id="gridProduct"
          />
        </AnchorPane>
        <GridPane fx:id="gridStockImport" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="15.0"
          AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="60.0" visible="false">
          <columnConstraints>
            <ColumnConstraints percentWidth="50" />
            <ColumnConstraints percentWidth="50" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
            <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
            <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
            <RowConstraints />

          </rowConstraints>
          <Label styleClass="lb-info-requerido" text="%txt_importing_request_name" GridPane.columnIndex="0"
            GridPane.rowIndex="0" />
          <TextField fx:id="txtStockImportingName" styleClass="campo" GridPane.columnIndex="0"
            GridPane.rowIndex="1">
            <GridPane.margin>
              <Insets top="10.0" bottom="10" />
            </GridPane.margin>
          </TextField>
          <Label styleClass="lb-info-requerido" text="%txt_stock_import" GridPane.columnIndex="0"
            GridPane.rowIndex="2" />
          <AnchorPane GridPane.columnSpan="2" GridPane.rowIndex="3">
            <AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <TableView fx:id="tbProductImporting" tableMenuButtonVisible="true"
                AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0">
                <columns>
                  <!--<TableColumn fx:id="colIdInOrder" maxWidth="50.0" minWidth="50.0"-->
                  <!--prefWidth="50.0" text="Id" />-->
                  <TableColumn fx:id="colProductNameImporting" maxWidth="900.0" minWidth="150.0"
                    prefWidth="150.0" text="%txt_product_name" />
                  <TableColumn fx:id="colDescriptionImporting" maxWidth="900.0" minWidth="150.0"
                    prefWidth="150.0" text="%txt_description" />
                  <TableColumn fx:id="colPriceImporting" maxWidth="900.0" minWidth="150.0"
                    prefWidth="250.0" text="%txt_price" />
                  <TableColumn fx:id="colNumberAdding" maxWidth="900.0" minWidth="150.0"
                    prefWidth="250.0" text="%txt_count" />
                </columns>
                <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
              </TableView>
            </AnchorPane>
          </AnchorPane>
        </GridPane>
      </StackPane>
      <GridPane fx:id="nodeAdd" visible="false" hgap="10.0" >
        <columnConstraints>
          <ColumnConstraints percentWidth="50" />
          <ColumnConstraints percentWidth="50" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
          <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
          <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
          <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
          <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
          <RowConstraints maxHeight="150.0" minHeight="100.0" prefHeight="150.0" />
        </rowConstraints>
        <children>
          <Label styleClass="lb-info-requerido" text="%txt_product_name" GridPane.columnIndex="0"
            GridPane.rowIndex="0" />
          <TextField fx:id="txtProductName" styleClass="campo" visible="true" GridPane.columnIndex="0"
            GridPane.rowIndex="1" />

          <Label layoutX="10.0" layoutY="17.0" styleClass="lb-info-requerido" text="%txt_price"
            GridPane.columnIndex="1" GridPane.rowIndex="0" />
          <TextField fx:id="txtPrice" layoutX="10.0" layoutY="40.0" styleClass="campo" visible="true"
            GridPane.columnIndex="1" GridPane.rowIndex="1" />

          <Label styleClass="lb-info" text="Loại sản phẩm" GridPane.columnIndex="0"
            GridPane.rowIndex="2"  />
          <HBox GridPane.columnIndex="0"
            GridPane.rowIndex="3" >
            <ComboBox fx:id="cbProductType" >
              <HBox.margin><Insets right="10"/></HBox.margin>
            </ComboBox>
            <Button text="Thêm mới" fx:id="btAddProductType" onAction="#addProductTypeAction"/>
          </HBox>

          <Label styleClass="lb-info" text="%txt_description" GridPane.columnIndex="1"
            GridPane.rowIndex="2" />
          <TextField fx:id="txtDescription" styleClass="campo" GridPane.columnIndex="1"
            GridPane.rowIndex="3" />

          <Label styleClass="lb-info" text="%txt_product_image" GridPane.columnIndex="0"
            GridPane.rowIndex="4" />
          <HBox GridPane.columnIndex="0"
            GridPane.rowIndex="5">
            <Button text="Chọn ảnh" fx:id="btUploadImage" onAction="#chooseImage"/>
            <Label fx:id="lbProductImage"  maxWidth="150" maxHeight="150">
              <HBox.margin>
                <Insets left="20"/>
              </HBox.margin>
            </Label>
          </HBox>


        </children>
      </GridPane>
    </StackPane>

    <AnchorPane prefHeight="50.0" styleClass="box-menus-subtela" AnchorPane.bottomAnchor="5.0"
      AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
      <children>
        <HBox AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="5.0">
          <StackPane>
            <Button id="" fx:id="btBackToList" mnemonicParsing="false"
              onAction="#goToProductList" prefHeight="40.0" prefWidth="200.0" style=""
              text="%txt_back">
              <styleClass>
                <String fx:value="menus-acoes"/>
                <!--<String fx:value="acao-editar" />-->
                <String fx:value="menu-blue"/>
              </styleClass>
              <StackPane.margin>
                <Insets right="30.0"/>
              </StackPane.margin>
            </Button>
          </StackPane>

          <StackPane>
            <Button fx:id="btEditImportingSession" contentDisplay="TOP" mnemonicParsing="false"
              onAction="#editImportingSession" prefHeight="40.0" prefWidth="230.0" style=""
              text="%txt_take_to_importing_session" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
              <styleClass>
                <String fx:value="menus-acoes"/>
                <!--<String fx:value="acao-editar"/>-->
                <String fx:value="menu-orange"/>
              </styleClass>
            </Button>
            <Button id="" fx:id="btCreateImportingSession" contentDisplay="TOP"
              mnemonicParsing="false" onAction="#createImportingSession" prefHeight="40.0"
              prefWidth="230.0" text="%txt_importing_request"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <styleClass>
                <String fx:value="menus-acoes"/>
                <!--<String fx:value="acao-salvar"/>-->
                <String fx:value="menu-green"/>
              </styleClass>
            </Button>
            <Button id="" fx:id="btCreateProduct" mnemonicParsing="false"
              onAction="#createProduct" prefHeight="40.0" prefWidth="200.0" style=""
              text="Thêm mới">
              <styleClass>
                <String fx:value="menus-acoes"/>
                <!--<String fx:value="acao-editar" />-->
                <String fx:value="menu-green"/>
              </styleClass>
              <StackPane.margin>
                <Insets right="30.0"/>
              </StackPane.margin>
            </Button>
          </StackPane>
        </HBox>

        <Label fx:id="lbLegend" layoutX="36.0" layoutY="17.0" styleClass="lb-info-requerido"
          text="%txt_required_fields" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.topAnchor="5.0"  />
      </children>
    </AnchorPane>
  </children>
  <stylesheets>
    <URL value="@../../css/default.css" />
    <URL value="@../../css/custom.css" />
  </stylesheets>
</fx:root>
