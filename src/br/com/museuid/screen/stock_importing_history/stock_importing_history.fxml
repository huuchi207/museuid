<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?><?import java.net.URL?><?import javafx.geometry.Insets?><?import javafx.scene.control.Button?><?import javafx.scene.control.ComboBox?><?import javafx.scene.control.Label?><?import javafx.scene.control.TableColumn?><?import javafx.scene.control.TableView?><?import javafx.scene.control.TextField?><?import javafx.scene.layout.AnchorPane?><?import javafx.scene.layout.ColumnConstraints?><?import javafx.scene.layout.GridPane?><?import javafx.scene.layout.HBox?><?import javafx.scene.layout.RowConstraints?><?import javafx.scene.layout.StackPane?>
<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.172-ea"
  xmlns:fx="http://javafx.com/fxml/1"
>
  <children>
    <HBox prefHeight="60.0" prefWidth="1207.0" styleClass="box-topo" AnchorPane.leftAnchor="0.0"
      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Label fx:id="lbTitle" alignment="BOTTOM_LEFT" layoutX="10.0"
          maxWidth="1.7976931348623157E308" styleClass="titulo" text="Título"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="445.0"
          AnchorPane.topAnchor="0.0" HBox.hgrow="ALWAYS" />
        <!--<TextField fx:id="txtSearch" prefHeight="50.0" prefWidth="417.0" promptText="%txt_search"-->
          <!--styleClass="search-txt-field" />-->
      </children>
    </HBox>
    <AnchorPane fx:id="apProductList" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="15.0"
      AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="60.0" visible="true">
      <HBox AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <Label styleClass="lb-info" text="Trạng thái:" alignment="CENTER_LEFT" prefHeight="30">
          <HBox.margin>
            <Insets right="10"/>
          </HBox.margin>
        </Label>
        <ComboBox fx:id="cbFilter" prefHeight="30" />
      </HBox>
      <TableView fx:id="tbStockImportHistory" tableMenuButtonVisible="true"
        AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0"
        AnchorPane.topAnchor="60.0">
        <columns>
          <TableColumn fx:id="colId" maxWidth="50.0" minWidth="50.0" prefWidth="50.0"
          text="STT" />
          <TableColumn fx:id="colImportingName" maxWidth="900.0" minWidth="150.0" prefWidth="150.0"
            text="%txt_importing_request_name" />
          <TableColumn fx:id="colImportingCreatedTime" maxWidth="900.0" minWidth="150.0"
            prefWidth="150.0" text="%txt_time_created" />
          <TableColumn fx:id="colImportingUpdatedTime" maxWidth="900.0" minWidth="150.0"
            prefWidth="150.0" text="%txt_time_updated" />
          <TableColumn fx:id="colImportingContent" maxWidth="900.0" minWidth="150.0"
            prefWidth="150.0" text="Thông tin lần nhập (Tên SP + số lượng nhập)" />
          <TableColumn fx:id="colImportingStatus" maxWidth="500.0" minWidth="100.0"
            prefWidth="100.0" text="%txt_status" />
          <!--<TableColumn fx:id="colStatus" maxWidth="900.0" minWidth="150.0"-->
          <!--prefWidth="250.0" text="%txt_status"/>-->
        </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
      </TableView>
    </AnchorPane>
    <GridPane fx:id="gridStockImport" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="15.0"
      AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="60.0" visible="false">
      <columnConstraints>
        <ColumnConstraints percentWidth="50" />
        <ColumnConstraints percentWidth="50" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
        <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
        <RowConstraints maxHeight="45.0" minHeight="30.0" prefHeight="30.0" />
        <RowConstraints />

      </rowConstraints>
      <Label styleClass="lb-info" text="%txt_importing_request_name" GridPane.columnIndex="0"
        GridPane.rowIndex="0" />
      <TextField fx:id="txtStockImportingName" styleClass="campo" GridPane.columnIndex="0"
        GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets top="10.0" bottom="10" />
        </GridPane.margin>
      </TextField>
      <Label styleClass="lb-info" text="%txt_stock_import" GridPane.columnIndex="0"
        GridPane.rowIndex="2" />
      <AnchorPane GridPane.columnSpan="2" GridPane.rowIndex="3">
        <AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <TableView fx:id="tbProductImporting" tableMenuButtonVisible="true"
            AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0">
            <columns>
              <!--<TableColumn fx:id="colIdInOrder" maxWidth="50.0" minWidth="50.0"-->
              <!--prefWidth="50.0" text="Id" />-->
              <TableColumn fx:id="colProductName" maxWidth="900.0" minWidth="150.0"
                prefWidth="150.0" text="%txt_product_name" />
              <TableColumn fx:id="colNumberAdding" maxWidth="900.0" minWidth="150.0"
                prefWidth="250.0" text="%txt_count" />
            </columns>
            <columnResizePolicy>
              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
          </TableView>
        </AnchorPane>
      </AnchorPane>
    </GridPane>

    <AnchorPane prefHeight="50.0" styleClass="box-menus-subtela" AnchorPane.bottomAnchor="5.0"
      AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
      <StackPane AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="0.0"
        AnchorPane.topAnchor="5.0">
        <HBox fx:id="boxActionList">
          <Button id="" fx:id="btCancel" mnemonicParsing="false" onAction="#cancel"
            prefHeight="40.0" prefWidth="200.0" style="" text="%txt_cancel">
            <styleClass>
              <String fx:value="menus-acoes" />
              <!--<String fx:value="acao-editar" />-->
              <String fx:value="menu-red" />
            </styleClass>
            <HBox.margin>
              <Insets right="30.0" />
            </HBox.margin>
          </Button>
          <Button fx:id="btEditImportingSession" contentDisplay="TOP" mnemonicParsing="false"
            onAction="#editImportingSession" prefHeight="40.0" prefWidth="230.0" style=""
            text="%txt_edit">
            <styleClass>
              <String fx:value="menus-acoes" />
              <!--<String fx:value="acao-editar"/>-->
              <String fx:value="menu-orange" />
            </styleClass>
          </Button>
        </HBox>
        <HBox fx:id="boxActionDetail">
          <Button id="" fx:id="btBackToList" mnemonicParsing="false" onAction="#goToHistoryList"
            prefHeight="40.0" prefWidth="200.0" style="" text="%txt_back">
            <styleClass>
              <String fx:value="menus-acoes" />
              <!--<String fx:value="acao-editar" />-->
              <String fx:value="menu-blue" />
            </styleClass>
            <HBox.margin>
              <Insets right="30.0" />
            </HBox.margin>
          </Button>
          <Button id="" fx:id="btUpdate" contentDisplay="TOP" mnemonicParsing="false"
            onAction="#updateImportingSession" prefHeight="40.0" prefWidth="230.0" style=""
            text="%txt_update" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <styleClass>
              <String fx:value="menus-acoes" />
              <!--<String fx:value="acao-salvar"/>-->
              <String fx:value="menu-green" />
            </styleClass>
          </Button>
        </HBox>
      </StackPane>

      <Label fx:id="lbLegend" layoutX="36.0" layoutY="17.0" styleClass="lb-info-requerido"
         AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0"
        AnchorPane.topAnchor="5.0" />
    </AnchorPane>
  </children>
  <stylesheets>
    <URL value="@../../css/default.css" />
    <URL value="@../../css/custom.css" />
  </stylesheets>
</fx:root>
