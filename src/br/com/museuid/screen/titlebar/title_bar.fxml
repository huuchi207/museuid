<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="br.com.museuid.screen.titlebar.TitlebarController"
            prefHeight="30.0" prefWidth="600.0" stylesheets="@../../css/login.css" >
  <HBox styleClass="box-janela" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0">
    <children>
      <Button mnemonicParsing="false" onAction="#minimize" styleClass="minimizar" text="Minimizar" />
      <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#close" styleClass="close" text="Fechar" />
    </children>
  </HBox>
</AnchorPane>
