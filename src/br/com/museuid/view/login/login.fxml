<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.PasswordField?><?import javafx.scene.control.TextField?><?import javafx.scene.image.ImageView?><?import javafx.scene.layout.AnchorPane?><?import javafx.scene.layout.HBox?><?import javafx.scene.layout.StackPane?><?import javafx.scene.layout.VBox?><?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="1000.0" styleClass="background"
  stylesheets="@../../css/login.css" xmlns="http://javafx.com/javafx/8.0.172-ea"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.museuid.view.login.LoginController">
  <children>
    <VBox alignment="CENTER" fillWidth="false" prefHeight="619.0" prefWidth="445.0" spacing="15.0"
      AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
      AnchorPane.topAnchor="0.0">
      <children>
        <ImageView fitHeight="100.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true"
          styleClass="logo" />
        <AnchorPane prefHeight="240.0" prefWidth="400.0">
          <children>
            <AnchorPane layoutX="-38.0" layoutY="-39.0" prefHeight="289.0" prefWidth="511.0"
              styleClass="box-login" AnchorPane.bottomAnchor="-10.0" AnchorPane.leftAnchor="-38.0"
              AnchorPane.rightAnchor="-73.0" AnchorPane.topAnchor="-39.0">
              <children>
                <TextField fx:id="tfUser" layoutX="68.0" layoutY="65.0" prefHeight="40.0"
                  prefWidth="336.0" styleClass="nome" AnchorPane.leftAnchor="20.0"
                  AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="60.0">
                  <font>
                    <Font size="14.0" />
                  </font>
                </TextField>
                <PasswordField fx:id="pfPass" layoutX="68.0" layoutY="120.0" prefHeight="40.0"
                  prefWidth="333.0" styleClass="senha" AnchorPane.leftAnchor="20.0"
                  AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="110.0">
                  <font>
                    <Font size="14.0" />
                  </font>
                </PasswordField>
                <Button layoutX="73.0" layoutY="179.0" mnemonicParsing="false" onAction="#login"
                  prefHeight="39.0" prefWidth="368.0" styleClass="bt-logar" text="%txt_log_in"
                  textFill="WHITE" AnchorPane.bottomAnchor="71.0" AnchorPane.leftAnchor="73.0"
                  AnchorPane.rightAnchor="72.0">
                  <font>
                    <Font size="16.0" />
                  </font>
                </Button>
                <Button layoutX="378.0" layoutY="239.0" prefHeight="17.0" prefWidth="103.0"
                  text="%txt-forgot-pass" textAlignment="RIGHT" textFill="#10cbe4"
                  styleClass="no-border-bt" onAction="#forgotPassword"/>
                <Label alignment="CENTER" layoutX="162.0" layoutY="21.0" styleClass="login-txt"
                  text="%txt_log_in" AnchorPane.leftAnchor="162.0" AnchorPane.rightAnchor="161.0"
                  AnchorPane.topAnchor="21.0">
                  <font>
                    <Font size="18.0" />
                  </font>
                </Label>
              </children>
            </AnchorPane>
            <StackPane layoutY="-3.0" prefHeight="40.0" prefWidth="248.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <children>
                <ImageView fitHeight="61.0" fitWidth="271.0" pickOnBounds="true"
                  preserveRatio="true" styleClass="login-img" />
              </children>
            </StackPane>
          </children>
        </AnchorPane>
        <Label fx:id="lbErrorLogin" alignment="CENTER" prefHeight="29.0" prefWidth="433.0"
          styleClass="error">
          <font>
            <Font size="14.0" />
          </font>
        </Label>
      </children>
    </VBox>
    <HBox styleClass="box-janela" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Button mnemonicParsing="false" onAction="#minimize" styleClass="minimizar"
          text="Minimizar" />
        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#close"
          styleClass="close" text="Fechar" />
      </children>
    </HBox>
  </children>
</AnchorPane>
